{
  "name": "xpath-dom",
  "version": "0.2.2",
  "description": "Easily use XPath with the DOM",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/johannhof/xpath-dom.git"
  },
  "scripts": {
    "build": "webpack index.js build/xpath-dom.js --debug --output-pathinfo --output-library=xpath --output-library-target=umd",
    "start": "webpack --watch index.js build/xpath-dom.js --debug --output-pathinfo --output-library=xpath --output-library-target=umd",
    "production": "webpack index.js dist/xpath-dom.min.js -p --output-library=xpath --output-library-target=umd",
    "test": "testem ci --port 8080"
  },
  "keywords": [
    "xpath",
    "dom",
    "evaluate",
    "html",
    "nodes"
  ],
  "author": "Johann Hofmann <mail@johann-hofmann.com>",
  "license": "MIT",
  "devDependencies": {
    "chai": "^3.2.0",
    "mocha": "^2.2.5",
    "testem": "^0.9.2",
    "webpack": "^1.12.0"
  }
}
